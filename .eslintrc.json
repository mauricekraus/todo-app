{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals": {
    "Todo": false,
    "State": false
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "quotes": [2, "single", {
      "avoidEscape": true
    }],
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "no-extra-semi": "error",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "eol-last": "off"
  }
}